		"test": "yarn run fr:test"
	},
	"workspaces": [
		"packages/*"
	],
	"devDependencies": {
		"@types/node": "^18.6.4",
		"eslint": "^8.21.0",
		"prettier": "^2.7.1",
		"typescript": "^4.7.4"
	}
}
