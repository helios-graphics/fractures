{
	"author": "Chris Puska <theta@hey.com>",
	"private": true,
	"scripts": {
		"docs:build": "yarn --cwd packages/fractures-dev next build",
		"docs:dev": "yarn --cwd packages/fractures-dev next",
		"docs:export": "yarn --cwd packages/fractures-dev next build && next export -o dist",
